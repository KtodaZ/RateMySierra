{
  "manifest_version": 2,
  "name": "RateMySierra",
  "description": "Links rate my professor to Sierra College's MySierra website",
  "version": "1.0",

  "author": "Kyle Szombathy",
  "author": "William Hexberg",
/*
  "omnibox": {
    "keyword": "SRMP"
  },
*/
  "icons": {
    "128": "icon.png"
  },

  "page_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "permissions": [
    "tabs",
    //"contextMenus",
    "http://www.ratemyprofessors.com/" // May not be needed
  ],

  "content_scripts": [
    {
      "matches": ["https://banprodssb.sierracollege.edu:8810/PROD/pw_sigsched.p_process"], //So scripts only runs on Sierra website
      "js": ["jquery-2.1.3.js", "main.js"]
    }
  ]
}